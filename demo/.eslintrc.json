{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "plugin:prettier/recommended"
    ],
    "settings": {
        "import/resolver": {
            "alias": {
                "map": [
                    ["@", "./src"],
                    ["@Component", "./src/components"],
                    ["@Image", "./src/assets/images"]
                ]
            }
        }
    }
}
